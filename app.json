        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.miana_n.taskly"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b6a6b5c5-6baf-4311-bb2c-1296745e9ec5"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
